{
  "name": "react-editable-fields",
  "version": "1.0.0",
  "description": "React components for inline-editing of forms",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "scripts": {
    "build": "npm run build-ts",
    "build-ts": "rm -rf dist/* && tsc --pretty",
    "build-background-watch": "tsc --watch --pretty",
    "test-watch": "npm run build-background-watch & mocha --watch --reporter spec --require test/setupBrowser.js dist/**.js",
    "test-spec": "mocha dist/**/*.spec.js -R spec --bail",
    "test-raw": "mocha --debug --reporter spec --require test/setupBrowser.js --recursive dist/test",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --debug --reporter spec --require test/setupBrowser.js --recursive dist/test",
    "test": "npm run build && npm run --silent test-cov",
    "prepublish": "typings install && npm run build",
    "devServer": "npm run build-background-watch & beefy --live --index ./tools/devServer/public/index.html dist/tools/devServer/main.js:bundle.js",
    "storybook": "start-storybook -p 9001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobdr/react-editable-fields.git"
  },
  "keywords": [
    "react",
    "form",
    "editable",
    "xeditable"
  ],
  "author": "Jacob Roberts <jacobdr@gmail.com>",
  "license": "SEE LICENSE IN LICESNSE",
  "bugs": {
    "url": "https://github.com/jacobdr/react-editable-fields/issues"
  },
  "homepage": "https://github.com/jacobdr/react-editable-fields#readme",
  "peerDependencies": {
    "react": "0.14.x",
    "react-dom": "0.14.x"
  },
  "dependencies": {
    "react-bootstrap": "^0.28.4",
    "source-map-support": "^0.4.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.6.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "i": "^0.3.4",
    "istanbul": "^0.4.2",
    "jsdom": "^8.2.0",
    "live-server": "^0.9.2",
    "mocha": "^2.4.5",
    "npm": "^3.8.5",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.8",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.1",
    "ts-node": "^0.7.1",
    "tsify": "^0.14.1"
  }
}
